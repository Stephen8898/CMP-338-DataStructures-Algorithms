
public class ArrayBasedList <I extends Comparable< ? super I >>implements ListInterface<I>  {

	private BetterArray<I> b = new BetterArray<I>(); 
	
	@Override
	public int size() {
		// TODO Auto-generated method stub
		return  b.size();
	}

	@Override
	public boolean isEmpty() {
		// TODO Auto-generated method stub
		return b.isEmpty();
	}

	@Override
	public void add(Comparable element) {
		// TODO Auto-generated method stub
			b.addElement((I) element);
	}

	@Override
	public boolean add(Comparable element, int index) {
		// TODO Auto-generated method stub
		return b.insertElement((I) element, index);
	}

	@Override
	public void addSorted(Comparable element) {
		// TODO Auto-generated method stub
		b.insertElementSorted((I) element);
	}

	@Override
	public I get(int index) {
		// TODO Auto-generated method stub
		return b.getElement(index);
	}

	@Override
	public I replace(Comparable element, int index) {
		// TODO Auto-generated method stub
		return b.replaceElement((I) element, index);
	}

	@Override
	public boolean remove (int index) {
		return b.removeElement(index);
	}

	@Override
	public void removeAll() {
		// TODO Auto-generated method stub
		b.resetArray();
	}

}
