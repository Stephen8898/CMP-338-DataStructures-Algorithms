import java.util.Iterator;

public class TestStack {
	
	private static void showQueue(ArrayBasedQueue<String> abq) {
		Iterator<String> myIterator = abq.iterator();
		
		String s = new String("{");

		while (myIterator.hasNext()) {
			s = s + myIterator.next();
			if (myIterator.hasNext()) {
				s = s + ", ";
			}
		}
		s = s + "}";
		
		System.out.println(s);
	}
	

	public static void main (String[] args) {
		ArrayBasedQueue<String> abq = new ArrayBasedQueue<String>();
		
		
		for ( int i = 1 ; i < 50 ; i++ ) {
			String s = new String("Element " + i);
			abq.enqueue(s);
		}
		
		showQueue(abq);
		
		String s = abq.dequeue();
		System.out.println(s);
		
		showQueue(abq);
		
		s = abq.dequeue();
		System.out.println(s);
		
		showQueue(abq);
		
		s = abq.dequeue();
		System.out.println(s);
		
		showQueue(abq);
		
		
		for ( int i = 50 ; i < 104 ; i++ ) {
			s = new String("Element " + i);
			abq.enqueue(s);
		}
		
		showQueue(abq);

	}
	
	
}
